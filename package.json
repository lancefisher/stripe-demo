    "lint": "eslint client server || true",
    "deploy": "npm run build && modulus deploy --project-name stripe-demo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lancefisher/stripe-demo"
  },
  "keywords": [
    "react",
    "reactjs",
